{
  "link_account": {
    "command": {
      "label": "hesap_bagla",
      "description": "Hesap bağlama işlemi.",
      "options": {
        "username": {
          "label": "kullanıcı_adı",
          "description": "Bağlanacak Minecraft kullanıcı adı."
        }
      }
    },
    "messages": {
      "minecraft": {
        "confirmation_code": {
          "text": "Doğrulama kodunuz <white><bold><hover:show_text:'Kodu kopyalamak için tıklayın'><click:suggest_command:'{confirmation_code}'>{confirmation_code}</click></hover></bold></white>. Lütfen bağlantı işlemini tamamlamak için bu kodu Discord'a girin."
        },
        "success": {
          "text": "Minecraft hesabınız başarıyla Discord hesabınıza bağlandı"
        },
        "failure": {
          "text": "Minecraft hesabınızı bağlarken bir hata oluştu. Lütfen daha sonra tekrar deneyin."
        }
      },
      "user": {
        "success": {
          "message_type": "embed",
          "content": {
            "title": "Hesap Başarıyla Bağlandı",
            "description": "Minecraft hesabınız başarıyla Discord hesabınıza bağlandı.",
            "color": "GREEN"
          }
        },
        "failure": {
          "message_type": "embed",
          "content": {
            "title": "Hesap Bağlama Başarısız",
            "description": "Minecraft hesabınızı bağlarken bir hata oluştu. Lütfen daha sonra tekrar deneyin.",
            "color": "RED"
          }
        }
      },
      "log": {
        "success": {
          "message_type": "embed",
          "content": {
            "title": "Account Linked",
            "description": "{discord_user_mention} kullanıcısı Minecraft hesabını bağladı.",
            "color": "GREEN",
            "fields": [
              {
                "name": "Minecraft Kullanıcı Adı",
                "value": "{minecraft_username}"
              },
              {
                "name": "Minecraft UUID",
                "value": "{minecraft_uuid}"
              }
            ]
          }
        },
        "failure": {
          "message_type": "embed",
          "content": {
            "title": "Hesap Bağlama Başarısız",
            "description": "{discord_user_mention} kullanıcısı Minecraft hesabını bağlarken hata oluştu.",
            "color": "RED",
            "fields": [
              {
                "name": "Hata",
                "value": "{error_message}"
              }
            ]
          }
        }
      }
    },
    "modal": {
      "confirmation": {
        "title": "Hesap Bağlama Doğrulaması",
        "fields": {
          "code": {
            "style": "SHORT",
            "label": "Doğrulama Kodu",
            "placeholder": "Minecraft hesabınıza gönderilen doğrulama kodunu girin."
          }
        }
      }
    }
  },
  "withdraw_rewards": {
    "command": {
      "label": "ödül_çek",
      "description": "Ödüllerinizi çekin."
    },
    "messages": {
      "user": {
        "success": {
          "message_type": "embed",
          "content": {
            "title": "Ödül Çekme Başarılı",
            "description": "Ödülleriniz başarıyla çekildi.",
            "color": "GREEN"
          }
        },
        "failure": {
          "message_type": "embed",
          "content": {
            "title": "Ödül Çekme Başarısız",
            "description": "Ödüllerinizi çekerken bir hata oluştu. Lütfen daha sonra tekrar deneyin.",
            "color": "RED"
          }
        }
      },
      "log": {
        "success": {
          "message_type": "embed",
          "content": {
            "title": "Ödül Çekme Başarılı",
            "description": "{discord_user_mention} kullanıcısı ödüllerini başarıyla çekti.",
            "color": "GREEN"
          }
        },
        "failure": {
          "message_type": "embed",
          "content": {
            "title": "Ödül Çekme Başarısız",
            "description": "{discord_user_mention} kullanıcısı ödüllerini çekerken hata oluştu.",
            "color": "RED"
          }
        }
      }
    }
  },
  "ban": {
    "command": {
      "label": "yasakla",
      "description": "Sunucudan bir kullanıcıyı yasakla.",
      "options": {
        "user": {
          "label": "kullanıcı",
          "description": "Yasaklanacak kullanıcı."
        },
        "duration": {
          "label": "süre",
          "description": "Yasak süresi."
        },
        "reason": {
          "label": "sebep",
          "description": "Yasaklama sebebi."
        }
      }
    },
    "messages": {
      "user": {
        "success": {
          "message_type": "plain",
          "content": {
            "text": "{discurd_user_mention} kullanıcısı başarıyla yasaklandı."
          }
        }
      }
    }
  },
  "error": {
    "unknown_error": {
      "message_type": "embed",
      "content": {
        "title": "Bilinmeyen Hata",
        "description": "Bilinmeyen bir hata oluştu. Lütfen bunu geliştiricilere bildirin.",
        "color": "RED"
      }
    },
    "timeout_error": {
      "message_type": "embed",
      "content": {
        "title": "Zaman Aşımı Hatası",
        "description": "İşlem zaman aşımına uğradı. Lütfen daha sonra tekrar deneyin.",
        "color": "RED"
      }
    },
    "command_execution_error": {
      "message_type": "embed",
      "content": {
        "title": "Komut Çalıştırma Hatası",
        "description": "Komut çalıştırılırken bir hata oluştu.",
        "color": "RED",
        "fields": [
          {
            "name": "Hata",
            "value": "{error_message}"
          }
        ]
      }
    },
    "user_record_not_found": {
      "message_type": "embed",
      "content": {
        "title": "Kullanıcı Kaydı Bulunamadı",
        "description": "Kullanıcı kaydı bulunamadı.",
        "color": "RED",
        "fields": [
          {
            "name": "Kullanıcı Adı",
            "value": "{discord_username}"
          },
          {
            "name": "Kullanıcı ID",
            "value": "{discord_user_id}"
          }
        ]
      }
    },
    "account_already_linked": {
      "message_type": "embed",
      "content": {
        "title": "Hesap Zaten Bağlı",
        "description": "Hesap zaten bağlanmış durumda.",
        "color": "RED",
        "fields": [
          {
            "name": "Oyuncu Adı",
            "value": "{minecraft_username}"
          },
          {
            "name": "Oyuncu UUID",
            "value": "{minecraft_uuid}"
          }
        ]
      }
    },
    "account_not_linked": {
      "message_type": "embed",
      "content": {
        "title": "Hesap Bağlı Değil",
        "description": "Hesap bağlı değil.",
        "color": "RED"
      }
    },
    "player_not_online": {
      "message_type": "embed",
      "content": {
        "title": "Oyuncu Çevrimiçi Değil",
        "description": "Minecraft oyuncusu çevrimiçi değil.",
        "color": "RED",
        "fields": [
          {
            "name": "Oyuncu Adı",
            "value": "{minecraft_username}"
          },
          {
            "name": "Oyuncu UUID",
            "value": "{minecraft_uuid}"
          }
        ]
      }
    }
  }
}
