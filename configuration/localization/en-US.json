{
  "events": {
    "guild_boost": {
      "messages": {
        "log": {
          "success": {
            "message_type": "embed",
            "content": {
              "title": "Server Boosted",
              "description": "{discord_user_mention} has boosted the server.",
              "color": "GREEN"
            }
          }
        }
      }
    }
  },
  "commands": {
    "link_account": {
      "command": {
        "label": "link_account",
        "description": "Link your Minecraft account to your Discord account.",
        "options": {
          "username": {
            "label": "username",
            "description": "The Minecraft username to link."
          }
        }
      },
      "messages": {
        "minecraft": {
          "confirmation_code": {
            "text": "Your confirmation code is <white><bold><hover:show_text:'Click to copy the code'><click:suggest_command:'{confirmation_code}'>{confirmation_code}</click></hover></bold></white>. Please enter this code in Discord to complete the linking process."
          },
          "success": {
            "text": "Your Minecraft account has been successfully linked to your Discord account."
          },
          "failure": {
            "text": "Failed to link your Minecraft account. Please try again later."
          }
        },
        "user": {
          "success": {
            "message_type": "embed",
            "content": {
              "title": "Account Successfully Linked",
              "description": "Your Minecraft account has been successfully linked to your Discord account.",
              "color": "GREEN"
            }
          },
          "failure": {
            "message_type": "embed",
            "content": {
              "title": "Account Linking Failed",
              "description": "Failed to link your Minecraft account. Please try again later.",
              "color": "RED"
            }
          }
        },
        "log": {
          "success": {
            "message_type": "embed",
            "content": {
              "title": "Account Linked",
              "description": "{discord_user_mention} has linked their Minecraft account.",
              "color": "GREEN",
              "fields": [
                {
                  "name": "Minecraft Username",
                  "value": "{minecraft_username}"
                },
                {
                  "name": "Minecraft UUID",
                  "value": "{minecraft_uuid}"
                }
              ]
            }
          },
          "failure": {
            "message_type": "embed",
            "content": {
              "title": "Account Linking Failed",
              "description": "{discord_user_mention} failed to link their Minecraft account.",
              "color": "RED"
            }
          }
        }
      },
      "modal": {
        "confirmation": {
          "title": "Account Linking Confirmation",
          "fields": {
            "code": {
              "style": "SHORT",
              "label": "Confirmation Code",
              "placeholder": "Enter the confirmation code sent to your Minecraft account."
            }
          }
        }
      }
    },
    "withdraw_rewards": {
      "command": {
        "label": "withdraw_rewards",
        "description": "Withdraw your rewards."
      },
      "messages": {
        "user": {
          "success": {
            "message_type": "embed",
            "content": {
              "title": "Rewards Withdrawn",
              "description": "Your rewards have been successfully withdrawn.",
              "color": "GREEN"
            }
          },
          "failure": {
            "message_type": "embed",
            "content": {
              "title": "Withdrawal Failed",
              "description": "Failed to withdraw your rewards. Please try again later.",
              "color": "RED"
            }
          }
        },
        "log": {
          "success": {
            "message_type": "embed",
            "content": {
              "title": "Rewards Withdrawn",
              "description": "{discord_user_mention} has withdrawn their rewards.",
              "color": "GREEN"
            }
          },
          "failure": {
            "message_type": "embed",
            "content": {
              "title": "Withdrawal Failed",
              "description": "{discord_user_mention} failed to withdraw their rewards.",
              "color": "RED"
            }
          }
        }
      }
    },
    "ban": {
      "command": {
        "label": "ban",
        "description": "Ban a user from the server.",
        "options": {
          "user": {
            "label": "user",
            "description": "The user to ban."
          },
          "duration": {
            "label": "duration",
            "description": "The duration of the ban."
          },
          "reason": {
            "label": "reason",
            "description": "The reason for the ban."
          }
        }
      },
      "messages": {
        "user": {
          "success": {
            "message_type": "plain",
            "content": {
              "text": "User {discord_user_mention} has been banned successfully."
            }
          }
        }
      }
    },
    "suggest": {
      "command": {
        "label": "suggest",
        "description": "Suggest a feature."
      },
      "messages": {
        "minecraft": {
          "approve": {
            "text": "Your suggestion has been approved by {discord_staff_username}."
          },
          "reject": {
            "text": "Your suggestion has been rejected by {discord_staff_username}."
          }
        },
        "user": {
          "success": {
            "message_type": "embed",
            "content": {
              "title": "Suggestion Submitted",
              "description": "Your suggestion has been submitted successfully.",
              "color": "GREEN"
            }
          },
          "failure": {
            "message_type": "embed",
            "content": {
              "title": "Suggestion Submission Failed",
              "description": "Failed to submit your suggestion. Please try again later.",
              "color": "RED"
            }
          }
        },
        "log": {
          "success": {
            "message_type": "embed",
            "content": {
              "title": "Suggestion Submitted",
              "description": "{discord_user_mention} has submitted a suggestion.",
              "color": "GREEN",
              "fields": [
                {
                  "name": "Suggestion",
                  "value": "{suggestion}"
                }
              ]
            }
          },
          "failure": {
            "message_type": "embed",
            "content": {
              "title": "Suggestion Submission Failed",
              "description": "{discord_user_mention} failed to submit a suggestion.",
              "color": "RED"
            }
          },
          "approve": {
            "message_type": "embed",
            "content": {
              "title": "Suggestion Approved",
              "description": "{discord_user_mention}'s suggestion has been approved.",
              "color": "GREEN",
              "fields": [
                {
                  "name": "Moderator",
                  "value": "{discord_staff_user_mention}"
                },
                {
                  "name": "Reason",
                  "value": "{reason}"
                },
                {
                  "name": "Suggestion",
                  "value": "{suggestion}"
                }
              ]
            }
          },
          "reject": {
            "message_type": "embed",
            "content": {
              "title": "Suggestion Rejected",
              "description": "{discord_user_mention}'s suggestion has been rejected.",
              "color": "RED",
              "fields": [
                {
                  "name": "Moderator",
                  "value": "{discord_staff_user_mention}"
                },
                {
                  "name": "Reason",
                  "value": "{reason}"
                },
                {
                  "name": "Suggestion",
                  "value": "{suggestion}"
                }
              ]
            }
          }
        }
      },
      "menu": {
        "confirmation": {
          "approve": {
            "label": "Approve",
            "emoji": "✅",
            "style": "SUCCESS"
          },
          "reject": {
            "label": "Reject",
            "emoji": "❌",
            "style": "DANGER"
          }
        }
      },
      "modal": {
        "send": {
          "title": "Give a Suggestion",
          "fields": {
            "suggestion": {
              "style": "PARAGRAPH",
              "label": "Suggestion",
              "placeholder": "Enter your suggestion here."
            }
          }
        },
        "respond": {
          "title": "Respond to Suggestion",
          "fields": {
            "response": {
              "style": "PARAGRAPH",
              "label": "Response",
              "placeholder": "Enter your response here."
            }
          }
        }
      }
    },
    "wiki": {
      "command": {
        "label": "wiki",
        "description": "Get information from the wiki.",
        "options": {
          "query": {
            "label": "query",
            "description": "The query to search for."
          }
        }
      },
      "messages": {
        "user": {
          "success": {
            "message_type": "embed",
            "content": {
              "title": "{query} - Wiki Search Result",
              "description": "{result}",
              "color": "BLUE"
            }
          },
          "failure": {
            "message_type": "embed",
            "content": {
              "title": "Wiki Search Failed",
              "description": "Failed to retrieve the wiki information. Please try again later.",
              "color": "RED"
            }
          }
        }
      }
    }
  },
  "general": {
    "success": {
      "message_type": "embed",
      "content": {
        "title": "Success",
        "description": "The operation was successful.",
        "color": "GREEN"
      }
    },
    "failure": {
      "message_type": "embed",
      "content": {
        "title": "Failure",
        "description": "The operation failed. Please try again later.",
        "color": "RED",
        "fields": [
          {
            "name": "Error",
            "value": "{error_message}"
          }
        ]
      }
    }
  },
  "error": {
    "unknown_error": {
      "message_type": "embed",
      "content": {
        "title": "Unknown Error",
        "description": "An unknown error occurred. Please report this to the developers.",
        "color": "RED"
      }
    },
    "timeout_error": {
      "message_type": "embed",
      "content": {
        "title": "Timeout Error",
        "description": "The request timed out. Please try again later.",
        "color": "RED"
      }
    },
    "channel_not_found_error": {
      "message_type": "embed",
      "content": {
        "title": "Channel Not Found",
        "description": "The specified channel was not found.",
        "color": "RED"
      }
    },
    "command_execution_error": {
      "message_type": "embed",
      "content": {
        "title": "Command Execution Error",
        "description": "An error occurred while executing the command.",
        "color": "RED",
        "fields": [
          {
            "name": "Error",
            "value": "{error_message}"
          }
        ]
      }
    },
    "user_record_not_found": {
      "message_type": "embed",
      "content": {
        "title": "User Record Not Found",
        "description": "The user record was not found.",
        "color": "RED",
        "fields": [
          {
            "name": "Username",
            "value": "{discord_username}"
          },
          {
            "name": "User ID",
            "value": "{discord_user_id}"
          }
        ]
      }
    },
    "account_already_linked": {
      "message_type": "embed",
      "content": {
        "title": "Account Already Linked",
        "description": "The account has been already linked.",
        "color": "RED",
        "fields": [
          {
            "name": "Player Name",
            "value": "{minecraft_username}"
          },
          {
            "name": "Player UUID",
            "value": "{minecraft_uuid}"
          }
        ]
      }
    },
    "account_not_linked": {
      "message_type": "embed",
      "content": {
        "title": "Account Not Linked",
        "description": "The account is not linked.",
        "color": "RED"
      }
    },
    "player_not_online": {
      "message_type": "embed",
      "content": {
        "title": "Player Not Online",
        "description": "The minecraft player is not online.",
        "color": "RED",
        "fields": [
          {
            "name": "Player Name",
            "value": "{minecraft_username}"
          },
          {
            "name": "Player UUID",
            "value": "{minecraft_uuid}"
          }
        ]
      }
    }
  }
}
